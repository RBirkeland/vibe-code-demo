{
  "name": "vibe-code-demo",
  "version": "1.0.0",
  "description": "Todo app demo for multi-agent parallel development",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:watch": "vite --open --watch",
    "build": "vite build --minify",
    "build:watch": "vite build --watch",
    "preview": "vite preview --open",
    "clean": "rm -rf dist node_modules .vite",
    "clean:dist": "rm -rf dist",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint main.js",
    "lint:css": "stylelint styles.css",
    "lint:fix": "eslint main.js --fix && stylelint styles.css --fix",
    "lint:watch": "npm run lint:fix -- --watch",
    "type-check": "echo 'Run type checking if using TypeScript'",
    "test": "echo 'Configure test runner (e.g., vitest, jest)'",
    "format": "npm run lint:fix",
    "validate": "npm run lint && npm run type-check && npm run build",
    "start": "npm run dev"
  },
  "keywords": [
    "todo",
    "demo",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.54.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "vibe-kanban-web-companion": "^0.0.5"
  }
}
